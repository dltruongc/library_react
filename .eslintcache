es] = useState({\n    name: '',\n    phone: '',\n    email: '',\n    gender: '',\n    address: '',\n    username: '',\n    password: '',\n    avatar: ''\n  });\n\n  useEffect(() => {});\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    console.log(e);\n  }\n\n\n\n  return <div>\n    <div className=\"row h-100vh\">\n      <div className=\"col col-md-8\">\n        <div className=\"\">\n          <img className='' src={landing} alt=\"ITLib\"/>\n        </div>\n        <div className=\"bottom-left p-4 custom-img-overlay\">\n          <h1>Đăng ký tài khoản</h1>\n          <div>Tham gia cùng chúng tôi để trải nghiệm dịch vụ thư viện trực tuyến tốt nhất cho bạn</div>\n        </div>\n      </div>\n      <div className=\"col col-md-4 bg-white h-100 overflow-scroll\">\n        <div className=\"d-flex pt-lg-2 flex-column justify-content-around align-items-center\">\n          <img className='mh-200 ' src={logo} alt=\"ITLib\"/>\n          <Form className='mt-2 mb-4 w-100' onSubmit={handleSubmit}>\n            <h2 className='text-center'>Tạo tài khoản mới</h2>\n            <Form.Group controlId='formFullName'>\n              <Form.Label>Họ tên</Form.Label>\n              <Form.Control type=\"text\" value={formValues.name} placeholder=\"Họ và tên\" name='CN_TEN' />\n            </Form.Group>\n            <Form.Group controlId='formPhone'>\n              <Form.Label>Điện thoại</Form.Label>\n              <Form.Control type=\"text\" value={formValues.phone} placeholder=\"Số điện thoại\" name='CN_SDT' />\n            </Form.Group>\n            <Form.Group controlId='formBasicEmail'>\n              <Form.Label>Email</Form.Label>\n              <Form.Control type=\"text\" value={formValues.email} placeholder=\"Email\" name='CN_EMAIL' />\n            </Form.Group>\n            <Form.Group controlId='formBasicGender'>\n              <Form.Check \n                type='checkbox'\n                label='Giới tính nam'\n                defaultChecked={true}\n                name='gender'\n                value={formValues.gender}\n              />\n            </Form.Group>\n            <Form.Group controlId='formBasicAddress'>\n              <Form.Label>Địa chỉ</Form.Label>\n              <Form.Control type=\"text\" value={formValues.address} placeholder=\"Địa chỉ\" name='CN_DIACHI' />\n            </Form.Group>\n            <Form.Group controlId='formBasicUsername'>\n              <Form.Label>Tên tài khoản</Form.Label>\n              <Form.Control type=\"text\" value={formValues.username} placeholder=\"Tên tài khoản\" name='CN_TENTAIKHOAN' />\n            </Form.Group>\n            <Form.Group controlId=\"formBasicPassword\">\n              <Form.Label>Mật khẩu</Form.Label>\n              <Form.Control type=\"password\" value={formValues.password} placeholder=\"Mật khẩu\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"formBasicPassword\">\n              <Form.File \n                id=\"avatar\"\n                label=\"Chọn ảnh đại diện\"\n                data-browse=\"Chọn từ tệp\"\n                custom\n                value={formValues.avatar}\n              />\n            </Form.Group>\n            <Form.Group controlId=\"formBasicCheckbox\">\n              <div>\n              Đã có tài khoản?{' '}\n              <Link to='../login'>Đăng nhập</Link></div> \n            </Form.Group>\n            <Button size=\"lg\" block  variant=\"primary\" type=\"submit\">\n              Tạo ngay\n            </Button>\n          </Form>\n        </div>\n      </div>\n    </div>\n  </div>;\n}","/Users/dlt/Codes/library_client/src/hooks/NetworkProvider.js",["173"],"import React, { useState, useContext } from \"react\";\nimport { Button, Modal } from \"react-bootstrap\";\nimport { css } from \"@emotion/core\";\nimport { RingLoader } from \"react-spinners\";\nimport { Redirect } from 'react-router-dom';\nimport { AuthContext } from './AuthProvider';\nimport axios from 'axios';\nconst override = css`\n  display: block;\n  margin: 0 auto;\n  border-color: red;\n`;\n\nexport const NetworkContext = React.createContext({\n  setLoading: () => {}, \n  handleShow: () => {}, \n  closeNotify: () => {},\n  onRequest: (promise, onSuccess = (s) => {}, onError = (e) => {}) => {},\n  onMultiRequests: (promises, onSuccess = (s) => {}, onError = (e) => {}) => {}\n});\n\nexport default function NetworkProvider ({ children }) {\n  const [error, setError]       = useState(null);\n  const [loading, setLoading]   = useState(false);\n  const [expired, setExpired]   = useState(false);\n  const { logout }              = useContext(AuthContext);\n\n  const showNotify = (error) => { setError(error)};\n  const closeNotify = () => { setError(null)};\n\n  const onRequest = (promise, onData = (s) => {}, onError = (e) => {}) => {\n    setLoading(true);\n    promise\n      .then(res => {\n        if (res.status === 204) {\n          showNotify('Không có dữ liệu');\n        }\n        console.log(res);\n        onData(res.data);\n      })\n      .catch(error => {\n        if (error?.response) {\n          if (error?.response.data.expiredAt) {\n            showNotify('Phiên đăng nhập đã hết hạn');\n            logout();\n          }\n          if(error.response.data.message) {\n            showNotify(error.response.data.message);\n          }\n          onError(error);\n        }\n      })\n      .finally(() => {setLoading(false)});\n  };\n  \n  const onMultiRequests = (promises, onData = (s) => {}, onErrors = (e) => {}) => {\n    let errorsMsg = [];\n    setLoading(true);\n    promises\n      .then(axios.spread((...responses) => {\n        onData(responses.map(res => res.data));\n      }))\n      .catch(errors => {\n        if (errors && errors.length) {\n          errors.forEach(error => {\n            if(error.response.data.message) {\n              errorsMsg.push(error.response.data.message);\n            }\n          })\n          showNotify(errorsMsg.response.data.message);\n        }\n        onErrors(errors);\n      })\n      .finally(() => {setLoading(false)});\n  };\n  \n  function showModal() {\n    let show = error != null;\n\n    if (show) {\n      return <div>\n        <Modal\n          show={show}\n          onHide={closeNotify}\n          backdrop=\"static\"\n          keyboard={true}\n          centered\n        >\n          <Modal.Header closeButton>\n            <Modal.Title>Thông báo</Modal.Title>\n          </Modal.Header>\n          <Modal.Body>\n            {error}\n          </Modal.Body>\n          <Modal.Footer>\n            <Button variant=\"primary\" onClick={closeNotify}>\n              Close\n            </Button>\n          </Modal.Footer>\n        </Modal>\n      </div>\n    }\n  }\n\n  return <NetworkContext.Provider value={{ setLoading, showNotify, closeNotify, onRequest, onMultiRequests }}>\n    <div>\n      { loading \n        ? <div className=\"loader\">\n            <RingLoader \n              css={override}\n              size={120}\n              color={\"#123abc\"}\n              loading={loading}\n            />\n          </div> \n        : null\n      }\n      {showModal()}\n      { expired ? <Redirect to='/login' /> : null }\n      {children}\n    </div>\n  </NetworkContext.Provider>;\n}\n","/Users/dlt/Codes/library_client/src/hooks/AuthProvider.js",["174","175"],"import React, { useEffect, useState } from \"react\";\nimport { setToken } from '../base/remote';\n\nexport const AuthContext = React.createContext({\n  auth: {\n    token: null,\n    avatar: null,\n    name: null,\n    isAdmin: false\n  }, \n  onAuth: (authData) => {},\n  logout: () => {}\n});\n\nexport default function AuthProvider ({ children }) {\n\n  const [auth, setAuth] = useState({\n    ...JSON.parse(sessionStorage.getItem('auth')),\n  });\n  \n  const onAuth = (authData) => {\n    sessionStorage.setItem('auth', JSON.stringify(authData));\n    setAuth(authData);\n  }\n\n  const logout = () => {\n    sessionStorage.clear('token');\n    sessionStorage.clear('avatar');\n    sessionStorage.clear('name');\n    setAuth({ token: null, avatar: null, name: null });\n  };\n\n  return <AuthContext.Provider value={{auth, onAuth, logout}}>\n      {children}\n    </AuthContext.Provider>;\n}","/Users/dlt/Codes/library_client/src/components/Avatar.js",[],"/Users/dlt/Codes/library_client/src/pages/Profile.js",["176"],"import { useContext, useState, useEffect } from \"react\";\nimport { NetworkContext } from \"../hooks/NetworkProvider\";\nimport { remote } from '../base/remote';\nimport { AuthContext } from \"../hooks/AuthProvider\";\nimport { Redirect } from \"react-router-dom\";\n\nexport default function Profile () {\n  const {showNotify, onRequest} = useContext(NetworkContext);\n  const { auth } = useContext(AuthContext);\n\n  const [user, setUser] = useState();\n\n  useEffect(() => {\n    console.log('call API!!');\n    onRequest(remote.get('/me', {\n      headers: { 'Authorization': 'Bearer ' + auth.token }\n    }), (data) => {\n      setUser(data);\n    }, (error) => {\n      // console.error(error);\n      showNotify('Bạn chưa đăng nhập')\n    });\n  }, []);\n\n  return (<>\n    { auth.token ? JSON.stringify(user): <Redirect to='/login'/>}\n  </>);\n}","/Users/dlt/Codes/library_client/src/pages/Category.js",["177","178","179","180","181"],"import MainNav from \"../components/MainNav\";\nimport Footer from \"../components/Footer\";\nimport React, {useContext, useEffect, useState} from \"react\";\nimport {BASE_URL, remote} from \"../base/remote\";\nimport {Link} from \"react-router-dom\";\nimport { Form, Button, InputGroup } from 'react-bootstrap';\nimport {AiOutlineSearch} from 'react-icons/ai';\nimport { NetworkContext } from \"../hooks/NetworkProvider\";\n\nexport default function Category () {\n  \n  const [books, setBooks] = useState([]);\n  const [categories, setCategories] = useState([]);\n\n\n  const { showNotify, onRequest } = useContext(NetworkContext);\n\n  useEffect(() => {\n    const getBooks = remote.get('/books');\n    onRequest(getBooks, onData, onError);\n  }, []);\n\n\n  useEffect(() => {\n    const getCategories = remote.get('/categories');\n    onRequest(getCategories, (categories) => {setCategories(categories.map(c => c.CDS_TEN))}, onError);\n  }, []);\n\n  const filterBooksByCategory = (e) => {\n    console.log('he lo')\n    e.preventDefault();\n\n    const getBooks = remote.get('/books?category=' + e.target.value);\n    onRequest(getBooks, (data) => {\n      setBooks(data);\n    }, onError);\n  }\n\n  const onData = (data) => {\n    if (data.length === 0) {\n      showNotify('Không có kết quả phù hợp');\n    } else setBooks(data);\n  }\n\n  const onError = (error) => {\n    console.error(error);\n  }\n\n\n  const Cards = books.map((item) => (\n    <div key={item.S_MA} className='col-3 mt-3 pb-2'>\n      <div className='card'>\n        <div className='card-image'>\n          <img src={BASE_URL +item?.S_HINHANH} alt={item?.S_TIEUDE} />\n        </div>\n        <div className='card-body'>\n          <div className='card-title'>\n            {item?.S_TIEUDE}\n          </div>\n          {item.description}\n          <div className='card-buttons'>\n            <Button variant='primary'>Borrow</Button>\n            <Link to={`/books/${item.S_MA}`}>\n              <Button variant='outline-primary'>\n                Details\n              </Button>\n            </Link>\n          </div>\n        </div>\n      </div>\n    </div>\n  ));\n\n  const NoBook = () => <div>Không tìm thấy sách</div>;\n\n  return (<div>\n      <header >\n        <MainNav/>\n      </header>\n      <section className='section trending-section mt-4'>\n        <div className='wrapper w-1140'>\n          <div className='row'>\n            <div className='col'>\n              <div className=\"section-title\">Tìm theo chủ đề</div>\n            </div>\n              <div className=\"col\">\n                <Form.Group controlId=\"formGridState\">\n                  <Form.Control as=\"select\" defaultValue=\"Chọn...\" name='categoryInput' onChange={filterBooksByCategory}>\n                    <option selected disabled={true}>Chọn chủ đề</option>\n                    {categories?.map(c => <option value={c}>{c}</option>)}\n                  </Form.Control>\n                </Form.Group>\n              </div>\n          </div>\n          <div className='row mt-4'>{Cards}</div>\n        </div>\n      </section>\n    <Footer/>\n  </div>);\n}","/Users/dlt/Codes/library_client/src/pages/Type.js",["182","183","184"],"import MainNav from \"../components/MainNav\";\nimport Footer from \"../components/Footer\";\nimport React, {useContext, useEffect, useState} from \"react\";\nimport {BASE_URL, remote} from \"../base/remote\";\nimport {Link} from \"react-router-dom\";\nimport { Form, Button } from 'react-bootstrap';\nimport { NetworkContext } from \"../hooks/NetworkProvider\";\n\nexport default function Type () {\n  \n  const [books, setBooks] = useState([]);\n  const [types, setTypes] = useState([]);\n\n\n  const { showNotify, onRequest } = useContext(NetworkContext);\n\n  useEffect(() => {\n    const getBooks = remote.get('/books');\n    onRequest(getBooks, onData, onError);\n  }, []);\n\n\n  useEffect(() => {\n    const getTypes = remote.get('/types');\n    onRequest(getTypes, (types) => {setTypes(types.map(t => { return {id: t.LS_MA, typeName: t.LS_TEN}}))}, onError);\n  }, []);\n\n  const filterBooksByType = (e) => {\n    console.log('he lo')\n    e.preventDefault();\n\n    const getBooks = remote.get('/books?type=' + e.target.value);\n    onRequest(getBooks, (data) => {\n      setBooks(data);\n    }, onError);\n  }\n\n  const onData = (data) => {\n    if (data.length === 0) {\n      showNotify('Không có kết quả phù hợp');\n    } else setBooks(data);\n  }\n\n  const onError = (error) => {\n    console.error(error);\n  }\n\n\n  const Cards = books.map((item) => (\n    <div key={item.S_MA} className='col-3 mt-3 pb-2'>\n      <div className='card'>\n        <div className='card-image'>\n 